{
  "require": {
    "php": ">=7.2",
    "slim/slim": "4.*",
    "slim/psr7": "^1.0",
    "php-di/slim-bridge": "^3.0",
    "doctrine/annotations": "^1.10",
    "monolog/monolog": "^2.0",
    "guzzlehttp/guzzle": "^6.5",
    "rmccue/requests": ">=1.0",
    "ext-dom": "*",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-curl": "*",
    "ext-intl": "*"
  },
  "autoload": {
    "psr-0": {"Requests": "library/"},
    "psr-4": {
      "ProxyBank\\": "src/ProxyBank"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "mikey179/vfsstream": "~1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
